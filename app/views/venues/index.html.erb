<h1>Listing Venues</h1>

<table class="bordered-table zebra-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Phone</th>
    <th>Capacity</th>
    <th>Box Office Hours</th>
    <th>Seating</th>
    <th></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @venues.each do |venue| %>
    <tr>
      <td><%= venue.name %></td>
      <td><%= venue.address %></td>
      <td><%= venue.phone %></td>
      <td><%= venue.capacity %></td>
      <td><%= venue.hours %></td>
      <td><%= venue.seating %></td>
      <td><%= link_to 'Show', venue %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br />

