<h1>Interested in:</h1>

<div id ='concerts-list'>
<% @concertsusers = ConcertsUser.all %>

<% @concertsusers.each do |c| %>
  <% concert = Concert.find(c.concert_id) %>
  <%= link_to concert, :class => "btn btn-inverse btn-mini", :id=>c.id, :style => 'color: #FFFFFF' do %>
      <%= concert.artist.name%>, <%= l concert.date, :format=>:day %>
  <% end %>
<% end %>
</div>

